Host *
ForwardAgent yes
ForwardX11 no
ForwardX11Trusted no
StrictHostKeyChecking no
GSSAPIAuthentication no
ControlMaster auto
ControlPersist 10h
PasswordAuthentication no
ServerAliveInterval 60
TCPKeepAlive yes
#User mmoa
#Ciphers aes128-ctr,aes192-ctr,aes256-ctr,arcfour256,arcfour128,aes128-cbc,3des-cbc
#HostKeyAlgorithms ssh-ed25519-cert-v01@openssh.com,ssh-ed25519,ecdsa-sha2-nistp256-cert-v01@openssh.com,ecdsa-sha2-nistp384-cert-v01@openssh.com,ecdsa-sha2-nistp521-cert-v01@openssh.com,ssh-rsa-cert-v01@openssh.com,ecdsa-sha2-nistp256,ecdsa-sha2-nistp384,ecdsa-sha2-nistp521,ssh-rsa

# login
Host login
ProxyCommand ssh -F ssh.config -q narvi-install.cc.tut.fi -W %h:%p
Hostname narvi.cc.tut.fi

# Merope-admin
Host merope-admin.cc.tut.fi
        User mmoa
        IdentityFile    ~/.ssh/midas_rsa
        ForwardX11              yes

Host merope-admin
        Hostname merope-admin.cc.tut.fi
        User mmoa
        IdentityFile    ~/.ssh/midas_rsa

